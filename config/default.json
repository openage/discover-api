{
    "dbServer": {
        "database": "discover",
        "host": "mongodb://localhost:27017/oa_discover"
    },
    "webServer": {
        "url": "http://localhost:3007",
        "port": 3007
    },
    "queueServer": {
        "disabled": true
    },
    "support": {
        "name": "Open Age",
        "email": "support@openage.in"
    },
    "auth": {
        "provider": "directory"
    },
    "logger": {
        "console": {
            "level": "silly",
            "handleExceptions": true,
            "json": false,
            "colorize": true
        }
    },
    "providers": {
        "google": {
            "url": "https://maps.googleapis.com/maps/api/place/nearbysearch/json",
            "api_Key": "placeholder"
        },
        "directory": {
            "url": "https://dev.openage.in/directory/api"
        },
        "send-it": {
            "url": "https://dev.openage.in/sendIt/api"
        }
    },
    "formats": {
        "date": "DD-MM-YYYY"
    },
    "organization": {
        "timeZone": "IST"
    }
}
